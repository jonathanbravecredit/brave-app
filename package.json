{
  "name": "brave-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startDev": "ng serve -c=dev --port=4201",
    "startQA": "ng serve -c=qa --port=4202",
    "build": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build -c=dev",
    "buildQA": "node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build -c=qa",
    "buildProd": "NODE_ENV=production node --max_old_space_size=3072 ./node_modules/@angular/cli/bin/ng build --prod",
    "buildSmall": "ng build --prod",
    "buildTailwind": "tailwind build src/styles.css -o src/assets/styles/tailwind.css",
    "amplifyDev": "amplify env checkout dev",
    "amplifyQA": "amplify env checkout qa",
    "amplifyProd": "amplify env checkout production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "build-storybook-big": "node --max_old_space_size=2000 ./node_modules/@storybook/angular/bin/index.js --quiet -p 6006 -c .storybook",
    "chromatic": "npx chromatic --project-token 897995d581ea -d storybook-static/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.7",
    "@angular/common": "~11.2.7",
    "@angular/compiler": "~11.2.7",
    "@angular/core": "~11.2.7",
    "@angular/forms": "~11.2.7",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@aws-amplify/ui-angular": "^1.0.6",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@ngxs/logger-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "@popperjs/core": "^2.9.2",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "aws-amplify": "^3.3.26",
    "aws-sdk": "^2.883.0",
    "chart.js": "^2.9.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.6",
    "@angular/cli": "~11.2.6",
    "@angular/compiler-cli": "~11.2.7",
    "@babel/core": "^7.14.3",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.0",
    "codelyzer": "^6.0.0",
    "graphql": "^15.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "postcss": "^8.2.9",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.1.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "browser": {
    "crypto": false
  }
}